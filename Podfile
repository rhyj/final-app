target 'bookworm' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Google/SignIn'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  # Pods for bookworm

  target 'bookwormTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'bookwormUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
